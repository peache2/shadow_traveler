script/source = "extends CharacterBody2D

@export var speed = 300
@export var jump = -500
@export var bullet_node: PackedScene

var jump_count = 0

# Control de disparo
var can_shoot: bool = true
var shoot_cooldown: float = 0.3

var atacar: bool = false
var gravity = 950

@onready var animated_sprite_2d = $AnimatedSprite2D


func shoot():
	
	if bullet_node == null:
		push_error(\"No asignaste la escena del bullet en el inspector.\")
		return

	if not can_shoot:
		return

	can_shoot = false
	# Inicia cooldown
	await get_tree().create_timer(shoot_cooldown).timeout
	can_shoot = true

	# Crear bala
	var bullet = bullet_node.instantiate()

	# Posici칩n de salida (puedes cambiarla si quieres que salga de la mano)
	bullet.position = global_position

	# Direcci칩n basada en hacia d칩nde mira el personaje
	bullet.direction = Vector2.LEFT if animated_sprite_2d.flip_h else Vector2.RIGHT

	# A침adir bala a la escena
	get_tree().current_scene.add_child(bullet)


func _input(event):
	if event.is_action_pressed(\"shoot\"):
		shoot()

func _physics_process(delta):
	if not is_on_floor():
		velocity.y += gravity * delta
	else:
		jump_count = 0

	if not atacar:

		# SALTO
		if Input.is_action_just_pressed(\"ui_accept\") and jump_count < 2:
			jump_count += 1
			velocity.y = jump

		# MOVIMIENTO
		var direction = Input.get_axis(\"ui_left\", \"ui_right\")

	
		if Input.is_action_just_pressed(\"dash\"):
			$Dashtest.play()
			$AnimatedSprite2D.play(\"Dash\")
			$DashTimer.start()
			speed *= 5
			velocity.x = direction * speed
			
			
			
			
		if direction < 0:
			animated_sprite_2d.flip_h = true
		elif direction > 0:
			animated_sprite_2d.flip_h = false

		if is_on_floor():
			if direction == 0:
				animated_sprite_2d.play(\"Idle\")
			else:
				animated_sprite_2d.play(\"Run\")
		else:
			if velocity.y < 0:
				animated_sprite_2d.play(\"Jump\")
			else:
				animated_sprite_2d.play(\"Fall\")

		if direction:
			velocity.x = direction * speed
		else:
			velocity.x = move_toward(velocity.x, 0, speed)



		move_and_slide()
		
		
func _on_dash_timer_timeout() -> void:
	speed = 300
"

