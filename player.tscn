script/source = "extends CharacterBody2D

@export var speed = 300
@export var jump = -500
@export var bullet_node: PackedScene
@export var atacar = false
@onready var animated_sprite_2d = $AnimatedSprite2D
@onready var sword_collision = $SwordCollision/CollisionShape2D



var jump_count = 0
var gravity = 950

func _process(_delta):

	# DETECTAR ATAQUE
	if Input.is_action_just_pressed(\"Attack\") and not atacar:
		attack()

	if atacar:
		var total_frames = animated_sprite_2d.sprite_frames.get_frame_count(\"Attack\")
		var last_frame = total_frames - 1
		
		if animated_sprite_2d.animation == \"Attack\" \\
		and animated_sprite_2d.frame == last_frame \\
		and not animated_sprite_2d.is_playing():
			atacar = false
			sword_collision.disabled = true


func attack():
	atacar = true
	velocity.x = 0  
	animated_sprite_2d.play(\"Attack\")
	sword_collision.disabled = false


func _physics_process(delta):

	if not is_on_floor():
		velocity.y += gravity * delta
	else:
		jump_count = 0

	if atacar:
		move_and_slide()
		return

	# SALTO
	if Input.is_action_just_pressed(\"ui_accept\") and jump_count < 2:
		jump_count += 1
		velocity.y = jump

	# MOVIMIENTO
	var direction = Input.get_axis(\"ui_left\", \"ui_right\")

	if Input.is_action_just_pressed(\"dash\"):
		$Dashtest.play()
		$DashTimer.start()
		speed *= 5
		velocity.x = direction * speed

	# Girar sprite
	if direction < 0:
		animated_sprite_2d.flip_h = true
	elif direction > 0:
		animated_sprite_2d.flip_h = false

	# ANIMACIONES
	if is_on_floor():
		if direction == 0:
			animated_sprite_2d.play(\"Idle\")
		else:
			animated_sprite_2d.play(\"Run\")
	else:
		if velocity.y < 0:
			animated_sprite_2d.play(\"Jump\")
		else:
			animated_sprite_2d.play(\"Fall\")

	# APLICAR MOVIMIENTO
	if direction:
		velocity.x = direction * speed
	else:
		velocity.x = move_toward(velocity.x, 0, speed)

	move_and_slide()


func _on_dash_timer_timeout():
	speed = 300
"

